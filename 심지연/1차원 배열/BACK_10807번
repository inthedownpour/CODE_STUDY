#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	int N = 0;
	int a[100] = { 0, };

	scanf("%d", &N);
	for (int i = 0; i < N; i++) {
		scanf("%d", &a[i]);
	}
	int b = 0;
	scanf("%d", &b);

	int sum = 0;
	for (int j = 0; j < N; j++) {
		if (b == a[j]) {
			sum += 1;
		}
	}
	printf("%d", sum);

	return 0;
}
