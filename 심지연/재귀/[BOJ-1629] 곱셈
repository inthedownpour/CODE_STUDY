#include<iostream>
#include<stdio.h>
#include<algorithm>
#include<string.h>

//변수크기 조심, longlong으로 선언하기! 

using namespace std;

long long times(long long a, long long b, long long c) {
	if (b == 1) { 
		return a%c;
	}
	long long val = times(a, b / 2, c);
	val = val * val % c;
	if (b % 2 == 0) return val;
	return val * a % c;
}


int main(void) {
	int a, b, c;
	int res = 0;
	cin >> a >> b >> c;
	res = times(a, b, c);
	cout << res;
	return 0;
}
