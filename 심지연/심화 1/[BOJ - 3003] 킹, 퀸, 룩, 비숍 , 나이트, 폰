#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>
#include <string.h>
#include <stdlib.h>

int main() {
	int str[6] = {0, };
	int con[6] = { 0, };
	for (int i = 0; i < 6; i++) {
		scanf("%d", &str[i]);
	}

	if (str[0] != 1) {
		con[0] = 1 - str[0];
	}
	if (str[1] != 1) {
		con[1] = 1 - str[1];
	}
	if (str[2] != 2) {
		con[2] = 2 - str[2];
	}
	if (str[3] != 2) {
		con[3] = 2 - str[3];
	}
	if (str[4] != 2) {
		con[4] = 2 - str[4];
	}
	if (str[5] != 8) {
		con[5] = 8 - str[5];
	}

	for (int i = 0; i < 6; i++) {
		printf("%d ", con[i]);
	}

	return 0;
}
